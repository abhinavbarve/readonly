<!doctype html>
<html lang="en">

    <head>
        <meta charset="utf-8">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <title>Database Login | ReadOnly</title>


        <!-- Bootstrap core CSS -->
        <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.0/css/bootstrap.min.css" integrity="sha384-9aIt2nRpC12Uk9gS9baDl411NQApFmC26EwAOH8WgZl5MYYxFfc+NcPb1dKGj7Sk" crossorigin="anonymous">


        <!-- Custom styles for this template -->
        <link type="text/css" href="css/styles2.css" rel="stylesheet">
    </head>

    <body class="text-center">
        <form class="form-signin" method="POST" action="/publishArticle">
        
        
            <img class="mb-4" src="Images/R-O.png" alt="" width="72" height="72">
            
            <h1 class="h3 mb-3 font-weight-normal">Login To Continue</h1>
            
            <!-- Username -->
            <input type= "text" class="form-control" name="username" placeholder="Username" required autofocus autocomplete="off">
            
            <!-- password -->
            <input type="password" class="form-control" name="password" placeholder="Password" required autocomplete="off">
            
            <button type="submit" class="btn btn-lg btn-primary btn-block" type="submit">Login</button>
            
            <p class="mt-5 mb-3 text-muted">@ Read-Only</p>
        
        
        </form>
    </body>

</html>